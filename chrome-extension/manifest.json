{
  "manifest_version": 2,
  "name" : "Youtube Game",
  "description" : "Intergrates a simple game on youtube.com",
  "author" : "Keegan D'Souza",
  "version" : "0.3",
  "browser_action": {
    "default_icon": "icon.png"
},
  "content_scripts" : [
    {
      "matches" : [
        "*://www.youtube.com/*",
        "*://youtube.com/*"
      ],
      "run_at": "document_end",

      "js" : ["content.js"]
    }
  ]
}